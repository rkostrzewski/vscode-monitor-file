{
    "name": "vscode-monitor-file",
    "displayName": "vscode-monitor-file",
    "description": "An extension that forecefully reloads file.",
    "version": "0.0.1",
    "publisher": "rkostrzewski",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.monitorFile",
        "onCommand:extension.monitorFileCustom"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.monitorFile",
            "title": "Monitor File"
        }, {
            "command": "extension.monitorFileCustom",
            "title": "Monitor File with custom Interval"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}